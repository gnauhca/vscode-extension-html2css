{
	"name": "html2css",
	"displayName": "html2css",
	"description": "html2css",
	"publisher": "chaz",
	"author": "gnauhca",
	"icon": "assets/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gnauhca/vscode-extension-html2css.git"
	},
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:html2css.copyCss"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "html2css.copyCss",
				"title": "html2css"
			}
		],
		"keybindings": [
			{
				"command": "html2css.copyCss",
				"key": "ctrl+alt+h",
				"mac": "cmd+alt+h",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "html2css.copyCss",
					"group": "1_modification",
					"when": "editorHasSelection"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
